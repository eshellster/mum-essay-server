doctype html
html
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    title 원장리스트
  body
    ul
      table(border='2')
            thead
              tr
                th 캠퍼스
                th 원장
                th 전화번호

              - count = 0
              each i in members
                - count += 1
                tbody
                  td=i.displayName
                  td=i.username
                  td=i.phone

    ul
      h2='총인원 :'+ count
